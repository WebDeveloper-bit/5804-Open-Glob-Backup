{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText14",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "DelimitedText15",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText16",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ParentId as string,",
				"          Id as string,",
				"          Name as string,",
				"          BodyLength as integer,",
				"          ContentType as string,",
				"          CreatedById as string,",
				"          CreatedDate as string,",
				"          IsDeleted as boolean,",
				"          Description as string,",
				"          LastModifiedById as string,",
				"          LastModifiedDate as string,",
				"          OwnerId as string,",
				"          IsPrivate as boolean,",
				"          SystemModstamp as string,",
				"          {New Name} as string,",
				"          Links as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          caseNumber as integer,",
				"          id as string,",
				"          CreatedDate as string,",
				"          Status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"source1, source2 join(ParentId == source2@id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          caseNumber,",
				"          ParentId,",
				"          Id = source1@Id,",
				"          Name,",
				"          Links,",
				"          BodyLength,",
				"          ContentType,",
				"          CreatedById,",
				"          CreatedDate = source1@CreatedDate,",
				"          {New Name},",
				"          id = source2@id,",
				"          Status",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}